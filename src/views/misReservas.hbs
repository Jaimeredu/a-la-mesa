<div class="container bg-dark">
    <div class="row">
        <div class="col-md-12">
            <div class="well well-sm">
                <h1>Datos de las reservas</h1>
                {{#each misReservas}}
                <div class="col-md-3">
                    <div class="card">
                        <div class="card-body">
                            <h4 class="card-title">
                                Restaurante: {{nameRestaurante}}
                            </h4>
                            <p>Distribución: {{distribucion}}</p>
                            <p>Número de comensales: {{numComensales}}</p>
                            <p>Fecha de la reserva: {{fecha}} {{hora}}</p>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>